<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="2" author="Nataliya_Gordiets">
        <createTable tableName="conversions">
            <column name="conv_id" type="BIGSERIAL">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="conv_date" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="name_val1" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="name_val2" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="curs" type="double">
                <constraints nullable="false"/>
            </column>
            <column name="sum_val1" type="double">
                <constraints nullable="false"/>
            </column>
            <column name="sum_val2" type="double">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
